@model DataTable
@using System.Data
<main id="main" class="main">
    <span class="text-danger">@TempData["ErrorMessage"]</span>
    <span class="text-danger">@TempData["SuccessMessage"]</span>
    <div class="pagetitle">
        <h1>QuestionLevel List</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item">QuestionLevel</li>
                <li class="breadcrumb-item active">QuestionLevel List</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">

                <div class="card">
                    <div class="card-body text text-primary">
                        <div class="row">
                            <div class="col">
                                <h5 class="card-title">Search</h5>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col">
                                <label>QuestionLevel Name</label>
                                <div class="">
                                    <input type="text" class="form-control">
                                </div>
                            </div>
                            <div class="col">
                                <label>Min QuestionLevel</label>
                                <div class="">
                                    <input type="number" class="form-control">
                                </div>
                            </div>
                            <div class="col">
                                <label>Max QuestionLevel</label>
                                <div class="">
                                    <input type="number" class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col">
                                <label>From QuestionLevel Date</label>
                                <div class="col">
                                    <input type="date" class="form-control">
                                </div>
                            </div>
                            <div class="col">
                                <label>To QuestionLevel Date</label>
                                <div class="col">
                                    <input type="date" class="form-control">
                                </div>
                            </div>
                            <div class="col"></div>
                        </div>
                        <div class="row">
                            <div class="col my-3">
                                <a class="btn btn-success ">Search</a>
                                <a class="btn btn-danger ">Reset</a>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </section>

    <section class="section">
        <div class="row">
            <div class="col-lg-12">

                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-10">
                                <h5 class="card-title">QuestionLevel List</h5>
                            </div>
                            <div class="col"> <a class="btn btn-primary my-3" asp-controller="QuestionLevel" asp-action="AddQuestionLevel">Add QuestionLevel</a></div>
                        </div>



                        <!-- Table with stripped rows -->
                        <div class="table-responsive">
                            <table class="table table-striped datatable">
                                <thead>
                                    <tr>
                                        <th scope="col">QuestionLevelId</th>
                                        <th scope="col">QuestionLevel</th>
                                        @* <th scope="col">QuizID</th> *@
                                        <th scope="col">UserId</th>
                                        <th scope="col">Created</th>
                                        <th scope="col">Modified</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (DataRow dataRow in Model.Rows)
                                    {
                                        <tr>
                                            <td>@dataRow["QuestionLevelId"]</td>
                                            <td>@dataRow["QuestionLevel"]</td>                                         
                                            @* <td>@dataRow["QuizId"]</td> *@
                                            <td>@dataRow["UserId"]</td>
                                            <td>@dataRow["Created"]</td>
                                            <td>@dataRow["Modified"]</td>
                                            <td>
                                                <a asp-controller="QuestionLevel" asp-action="AddQuestionLevel" asp-route-QuestionLevelId="@dataRow["QuestionLevelId"]" class="btn btn-primary btn-sm" style="padding: 2px 6px; font-size: 12px;">
                                                    <i class="bi bi-pencil-fill"> Edit</i>
                                                </a>
                                                <a asp-controller="QuestionLevel" asp-action="QuestionLevelDelete" asp-route-QuestionLevelID="@dataRow["QuestionLevelID"]" class="btn btn-outline-danger btn-xs" onclick="return confirm('Are you sure you want to delete QuestionList?');">
                                                    <i class="bi bi-x-lg"></i>
                                                </a>
                                            </td>

                                        </tr>
                                    }
                                </tbody>

                            </table>
                        </div>
                        <!-- End Table with stripped rows -->

                    </div>
                </div>

            </div>
        </div>
    </section>

</main><!-- End #main -->